<div class="container mt-5">
  <div class="card shadow p-5">
    <h2>Users List</h2>
    <table class="table mt-3">
      <thead>
        <tr>
          <th>Name</th>
          <th>Address</th>
          <th>Pincode</th>
          <th>Phone Number</th>
          <th>Email</th>
          <th>Actions</th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |user| %>
          <tr>
            <td><%= user.name %></td>
            <td><%= user.address %></td>
            <td><%= user.pincode %></td>
            <td><%= user.phone_number %></td>
            <td><%= user.email %></td>
            <td><%= link_to "View profile" ,member_path(user),class:'btn btn-primary' %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
